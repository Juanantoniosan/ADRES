@model IEnumerable<PruebaAdres.Modelo.TipoBienoServicio>

@{
    ViewData["Title"] = "Index";
}

<h1>Tipos de Bienes o Servicios</h1>

<p>
    <a asp-action="Create">Crear</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Descripción
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TipoBienoServicioId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.TipoBienoServicioId">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.TipoBienoServicioId">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
